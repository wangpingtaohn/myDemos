        <activity android:name=".frgexit.FrgActivity"/>
        <activity android:name=".recycler.RecyclerViewActivity"/>
        <activity android:name=".statusbar.StatusBarActivity"/>
        <activity
                android:name=".keyboard.Keyboard2Activity"
                android:theme="@style/ActivityDialogTheme">
        </activity>
        <activity android:name=".map.MapJavaACtivity"/>
        <activity android:name=".keyboard.KeyboardActivity"/>
        <activity android:name=".kotlins.KotlinFunActivity"/>
        <activity android:name=".emoji.EmojiActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>